{
  "name": "brandforge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "lsof -ti:3000,3010 | xargs kill -9 || true; rm -rf .next && next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:sync": "npx prisma migrate deploy && npx prisma db seed",
    "db:status": "npx prisma migrate status",
    "db:bootstrap": "./scripts/db-bootstrap.sh",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:up": "bash ./scripts/check-docker.sh && docker-compose up -d",
    "db:down": "docker-compose down",
    "db:logs": "docker-compose logs -f",
    "verify:db": "npx tsx scripts/verify-db.ts",
    "db:reset": "prisma migrate reset",
    "worker": "npx tsx lib/jobs/worker.ts",
    "worker:start": "npx tsx lib/jobs/worker.ts",
    "verify:ping": "./scripts/verify-ping.sh",
    "verify:auth": "./scripts/verify-auth.sh",
    "verify:stage": "./scripts/verify-stage-run.sh",
    "verify:openai": "./scripts/verify-openai.sh",
    "verify:output": "./scripts/verify-output.sh",
    "smoke:core": "tsx -r tsconfig-paths/register scripts/smoke-test-core.ts",
    "verify:output:flow": "tsx -r tsconfig-paths/register scripts/verify-output-flow.ts",
    "verify:async:flow": "tsx -r tsconfig-paths/register scripts/verify-async-flow.ts",
    "verify:token:gating": "npx tsx scripts/verify-token-gating.ts",
    "verify:addon:flow": "tsx -r tsconfig-paths/register scripts/verify-addon-flow.ts",
    "checklist:audit": "tsx -r tsconfig-paths/register scripts/checklist-audit.ts",
    "verify:openai:config": "npx tsx scripts/verify-openai-config.ts",
    "verify:openai:modes": "./scripts/verify-openai-modes.sh",
    "verify:e2e:auth": "./scripts/verify-e2e-auth.sh",
    "verify:e2e:security": "./scripts/verify-e2e-security.sh",
    "verify:models": "./scripts/verify-models.sh",
    "verify:all": "npm run verify:ping && npm run verify:auth && npm run verify:stage && npm run verify:openai && npm run verify:output",
    "verify:env": "npx tsx scripts/verify-env.ts",
    "env:bootstrap": "npx tsx scripts/bootstrap-env.ts",
    "predev": "npm run env:bootstrap",
    "preworker:start": "npm run env:bootstrap",
    "verify:ui:job-flow": "npx tsx scripts/verify-ui-job-flow.ts",
    "verify:admin:v3": "tsx -r tsconfig-paths/register scripts/verify-admin-v3.ts",
    "verify:stripe": "npx tsx scripts/verify-stripe-config.ts",
    "verify:ai:providers": "npx tsx scripts/verify-ai-providers.ts",
    "verify:collaboration": "npx tsx scripts/verify-collaboration.ts",
    "verify:billing": "npx tsx scripts/verify-billing.ts",
    "healthcheck:local": "npx tsx scripts/healthcheck-local.ts",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@clerk/localizations": "^3.32.1",
    "@clerk/nextjs": "^6.36.5",
    "@node-saml/node-saml": "^5.1.0",
    "@prisma/client": "^6.19.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "archiver": "^7.0.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jose": "^6.1.3",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "otpauth": "^9.4.1",
    "playwright": "^1.57.0",
    "prisma": "^6.19.1",
    "qrcode": "^1.5.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "stripe": "^20.1.0",
    "svix": "^1.82.0",
    "tailwind-merge": "^3.4.0",
    "tsx": "^4.21.0",
    "uuid": "^13.0.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/archiver": "^7.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "tsx -r tsconfig-paths/register scripts/seed.ts"
  }
}
